{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
	"install": {
      "dependsOn": ["^install"]
    },
    "build": {
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },

    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}